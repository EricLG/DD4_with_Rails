<%# Sidebloc content%>
<% content_for :sidebloc do %>
  <%= render "sidebloc/feats_menu" %>
<% end %>

<div class="row">
  <h3 class="col-sm-12"><%= t("feats.title_list") %></h3>
</div>

<div class="row">
  <div class="col-sm-12">
    <%= render"form_search" %>
  </div>
</div>

<div class="row">
<% if @feats.empty? %>
  <p class="col-sm-5"><%= t('common.no_result') %></p>
<% else %>
  <div class="col-sm-12">
    <%= render "index_table", feats: @feats %>
  </div>
<% end %> <!-- Fin if -->
</div>
